{
  "name": "nestry-server",
  "version": "1.0.0",
  "private": true,
  "description": "全栈后台框架",
  "author": "",
  "license": "MIT",
  "prisma": {
    "schema": "./prisma",
    "seed": "ts-node ./prisma/seed.ts"
  },
  "scripts": {
    "dev": "pnpm run start:dev",
    "build": "cross-env NODE_ENV=production nest build --webpack --webpackPath webpack.config.js",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest build --webpack --webpackPath webpack.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "prisma:utils:generate": "node ./src/common/utils/prismaUtil.js -generate",
    "prisma:utils:update": "node ./src/common/utils/prismaUtil.js -update",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "prisma:reset": "npx prisma migrate reset --force",
    "prisma:seed": "npx prisma db push && ts-node ./prisma/seed.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "@nestjs/axios": "^3.1.3",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^6.3.0",
    "@prisma/client": "^6.0.1",
    "archiver": "^7.0.1",
    "axios": "^1.7.9",
    "bowser": "^2.11.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cos-nodejs-sdk-v5": "^2.14.6",
    "cron": "^3.3.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "generic-pool": "^3.9.0",
    "gradient-string": "^2.0.2",
    "helmet": "^8.0.0",
    "iconv-lite": "^0.6.3",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.5",
    "nestjs-throttler-storage-redis": "^0.5.1",
    "nodemailer": "^6.9.16",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.1",
    "svg-captcha": "^1.4.0",
    "swagger-ui-express": "^5.0.1",
    "systeminformation": "^5.23.10"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.15",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.1",
    "@swc/helpers": "^0.5.15",
    "@types/express": "^5.0.0",
    "@types/gradient-string": "^1.1.6",
    "@types/lodash": "^4.17.13",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.2",
    "@types/nodemailer": "^6.4.17",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.2",
    "@types/systeminformation": "^3.54.1",
    "@types/webpack-env": "^1.18.5",
    "@types/webpack-node-externals": "^3.0.4",
    "@vitest/coverage-v8": "^2.1.8",
    "cross-env": "^7.0.3",
    "eslint": "^9.16.0",
    "eslint-plugin-format": "^0.1.3",
    "prisma": "^6.0.1",
    "run-script-webpack-plugin": "^0.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "swc-loader": "^0.2.6",
    "terser-webpack-plugin": "^5.3.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2",
    "unplugin-swc": "^1.5.1",
    "vitest": "^2.1.8",
    "webpack": "^5.97.1",
    "webpack-node-externals": "^3.0.0"
  }
}
