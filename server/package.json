    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/throttler": "^5.2.0",
    "@prisma/client": "^5.16.1",
    "archiver": "^7.0.1",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "bowser": "^2.11.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cos-nodejs-sdk-v5": "^2.14.2",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "generic-pool": "^3.9.0",
    "helmet": "^7.1.0",
    "iconv-lite": "^0.6.3",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.10.2",
    "nestjs-throttler-storage-redis": "^0.4.4",
    "node-xlsx": "^0.24.0",
    "nodemailer": "^6.9.14",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.1",
    "svg-captcha": "^1.4.0",
    "swagger-ui-express": "^5.0.1",
    "systeminformation": "^5.22.11",
    "velocityjs": "^2.0.6"
