{
  "name": "nestry-server",
  "version": "0.0.1",
  "private": true,
  "description": "全栈后台框架",
  "author": "",
  "license": "MIT",
  "prisma": {
    "schema": "./prisma",
    "seed": "ts-node ./prisma/seed.ts"
  },
  "scripts": {
    "dev": "nest start -b swc -w --type-check",
    "build": "nest build -b swc",
    "start": "nest start -b swc",
    "start:dev": "nest start -b swc -w --type-check",
    "start:debug": "nest start -b swc --debug",
    "start:prod": "node dist/main",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "prisma:generate": "node ./src/common/utils/prismaUtil.js -generate",
    "prisma:update": "node ./src/common/utils/prismaUtil.js -update",
    "prisma:seed": "ts-node ./prisma/seed.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
