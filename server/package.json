{
  "name": "nest-server",
  "version": "0.0.1",
  "private": true,
  "description": "全栈后台框架",
  "author": "",
  "license": "MIT",
  "prisma": {
    "schema": "./prisma"
  },
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "node ./src/common/utils/prismaUtil.js -generate",
    "prisma:update": "node ./src/common/utils/prismaUtil.js -update",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.10",
    "@nestjs/core": "^10.3.10",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/throttler": "^5.2.0",
    "@prisma/client": "^5.16.1",
    "archiver": "^7.0.1",
    "axios": "^1.7.2",
    "bowser": "^2.11.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "generic-pool": "^3.9.0",
    "iconv-lite": "^0.6.3",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.10.2",
    "nestjs-throttler-storage-redis": "^0.4.4",
    "node-xlsx": "^0.24.0",
    "nodemailer": "^6.9.14",
    "redis": "^4.6.15",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.1",
    "svg-captcha": "^1.4.0",
    "swagger-ui-express": "^5.0.1",
    "systeminformation": "^5.22.11",
    "velocityjs": "^2.0.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.2",
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.3.10",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.9",
    "@types/nodemailer": "^6.4.15",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.2",
    "@types/systeminformation": "^3.54.1",
    "eslint": "^9.6.0",
    "eslint-plugin-format": "^0.1.2",
    "jest": "^29.7.0",
    "prisma": "^5.16.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
